@namespace("io.github.dfauth.avro")
protocol TestProtocol {

  enum Status {
    ACTIVE,
    INACTIVE,
    PENDING
  } = INACTIVE;

  record User {
    string name;
    int age;
    Status status;
    decimal(9,2) salary;
    union { null, string } email = null;
  }

  record CurrencyPair {
     string base;
     string alt;
  }

  record CurrencyPairV2 {
     string base;
     string alt;
  }

  record CurrencyPairV3 {
     string base;
     string alt;
     string anotherField;
  }

}